#!/opt/rhea/bin/rhea

val nums = [1, 2, 3];
render! nums;

nums[1] = [2.25, 2.5, 2.75];
render! nums;

nums[1][1] = "Test";
render! nums;
render! nums[1][2];

render! ~["Hello", "Konnichiwa", "Kamusta"];

render! [1, 2, 3] + [4, 5, 6];
render! [4, 5, 6] - [3, 2, 1];
render! [1, 2, 3] * [4, 5, 6];
render! [4, 5, 6] / [3, 2, 1];

render! [1, 2, 3] & [4, 5, 6];
render! [4, 5, 6] | [3, 2, 1];
render! [1, 2, 3] ^ [4, 5, 6];

render! [4, 5, 6] << [3, 2, 1];
render! [6, 5, 4] >> [-1, -2, -3];
